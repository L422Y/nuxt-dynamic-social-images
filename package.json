  "devDependencies": {
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/fabric": "^4.5.12",
    "eslint": "^8.27.0",
    "nuxt": "^3.0.0"
